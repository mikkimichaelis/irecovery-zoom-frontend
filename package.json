{
  "name": "irecovery-zoom",
  "version": "0.1.0",
  "private": true,
  "homepage": "/api/zoomapp/proxy",
  "scripts": {
    "start": "(npm run build); (node ./server.js);",
    "debug": "(npm run build); (node --inspect=0.0.0.0  ./server.js);",
